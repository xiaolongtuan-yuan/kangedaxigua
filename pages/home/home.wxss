/* pages/home/home.wxss */
.tuoyuan {
  height: 60rpx;
  width: 180rpx;
  padding-left: 30rpx;
}

.text1 {
  padding-left: 30rpx;
  font-size: 40rpx;
  color: white;
}

.remen1 {
  margin: 0rpx 30rpx;
  display: flex;
  justify-content: space-between;
}

.remen {
  display: flex;
  margin: 5rpx 0rpx;
  width: 33%;
  height: 100rpx;
  background-color: #efe9ce;
  border-radius: 10rpx;
  text-align: center;
}

.swip {
  margin: 0rpx 30rpx;
  height: 200rpx;
  display: flex;
  align-items: center;

}

swiper-item {
  border-radius: 20rpx;
}

.list {
  margin: 10rpx 30rpx;
  height: 70vh;
  background-color: rgba(255, 255, 255, 50%);
  border-radius: 20rpx;
}
scroll-view{
  height: 100%;
}
.list-text{
  display: flex;
  justify-content: space-between;
  height: 150rpx;
  margin: 10rpx 30rpx;
  border-bottom-style: solid;
  border-width: 3rpx;
  border-color: white;
}
.a{
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content:space-between;
}
.info{
  color: white;
}
.money{
  color: white;
}
image{
  height: 100%;
  width: 100%;
}